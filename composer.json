        "2amigos/yii2-transliterator-helper": "dev-master",
        "2amigos/yii2-ckeditor-widget": "~1.0",
        "zainiafzan/yii2-dropzone": "dev-master",
        "yiisoft/yii2-imagine": "^2.0",
        "intervention/image": "^2.3",
        "pastuhov/yii2-yml-catalog": "^0.3.3",
        "kartik-v/yii2-widget-select2": "@dev",
        "kartik-v/yii2-widget-rating": "*",
        "nizsheanez/yii2-asset-converter": "1.*",
        "kartik-v/yii2-widget-colorinput": "*",
        "corpsepk/yii2-yandex-market-yml": "@dev",
        "kartik-v/yii2-grid": "@dev",
        "kartik-v/yii2-mpdf": "dev-master",
        "kartik-v/yii2-editable": "@dev",
        "mihaildev/yii2-ckeditor": "*",
        "andig/php-html-parser": "dev-master",
        "voku/html2text": "^3.4",
        "sirgalas/yii2-wordpress-menu": "dev-master",
        "sintret/yii2-chat-adminlte": "dev-master",
        "phpdocumentor/reflection-docblock": "^4.1",
        "paquettg/php-html-parser": "^1.7",
        "yiisoft/yii2-redis": "^2.0@dev",
        "yiisoft/yii2-elasticsearch": "^2.0@dev",
        "yiisoft/yii2-mongodb": "^2.1@dev",
        "yii2tech/embedded": "^1.0@dev",
        "alcaeus/mongo-php-adapter": "^1.1@dev",
        "sokil/php-mongo": "dev-master",
        "fxp/composer-asset-plugin": "^1.4@dev",
        "dmstr/yii2-adminlte-asset": "^2.1",
        "cebe/markdown": "~1.2.0",
        "sirgalas/yii2-edit-translation": "dev-master",
        "mihaildev/yii2-elfinder": "*"
    },
    "require-dev": {
        "yiisoft/yii2-codeception": "*",
        "yiisoft/yii2-debug": "*",
        "yiisoft/yii2-gii": "*",
        "yiisoft/yii2-faker": "*"
    },
    "config": {
        "process-timeout": 1800
    },
    "extra": {
        "asset-installer-paths": {
            "npm-asset-library": "vendor/npm",
            "bower-asset-library": "vendor/bower"
        }
    },
    "scripts": {
        "post-install-cmd": "php init --env=Development --overwrite=n"
    }
}
